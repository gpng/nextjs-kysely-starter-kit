{
  "name": "nextjs-kysely",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate:create": "ts-node ./src/scripts/create-migration.ts",
    "db:migrate:up": "ts-node -r ./src/scripts/with-env.ts ./src/scripts/migrate.ts up && npm run db:schema:generate",
    "db:migrate:down": "ts-node -r ./src/scripts/with-env.ts ./src/scripts/migrate.ts down && npm run db:schema:generate",
    "db:schema:generate": "kysely-codegen --out-file ./src/types/kysely.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "kysely": "^0.26.3",
    "lucide-react": "^0.292.0",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "~3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.9.5",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-ui-react": "^4.18.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "kysely-codegen": "^0.11.0",
    "postcss": "^8.4.31",
    "prettier": "3.1.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
